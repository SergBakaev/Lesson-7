public class Plate {
    private int food;
    public Plate(int food){
        this.food = food;
    }

    boolean decreaseFood(int n){
        int noFood = food-n;
        if (noFood<0) return false;

        food-=n;
        return true;
    }

    public void info(){
        System.out.println("Plate " + food);
    }
    public void addFood(int food){
        this.food+=food;
    }

    public int getFood() {
        return food;
    }

    public void setFood(int food) {
        this.food = food;
    }
}